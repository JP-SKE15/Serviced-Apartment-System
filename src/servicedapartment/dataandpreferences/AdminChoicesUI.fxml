<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="444.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="servicedapartment.dataandpreferences.AdminChoicesController">
   <center>
      <VBox alignment="CENTER" prefHeight="355.0" prefWidth="600.0" spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="&lt;&lt; Preferences... &gt;&gt;">
               <font>
                  <Font name="Comic Sans MS" size="24.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" prefHeight="65.0" prefWidth="580.0" spacing="10.0">
               <children>
                  <Button fx:id="editButton" mnemonicParsing="false" onAction="#handleEdit" prefHeight="55.0" prefWidth="255.0" text="Edit Rental Rates">
                     <font>
                        <Font name="Comic Sans MS" size="18.0" />
                     </font>
                  </Button>
                  <Button fx:id="roomButton" mnemonicParsing="false" onAction="#handleEditRoom" prefHeight="55.0" prefWidth="255.0" text="Add/Remove Rooms">
                     <font>
                        <Font name="Comic Sans MS" size="17.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <Label text="&lt;&lt; Customer Data &gt;&gt;">
               <font>
                  <Font name="Comic Sans MS" size="24.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" layoutX="20.0" layoutY="91.0" prefHeight="65.0" prefWidth="580.0" spacing="10.0">
               <children>
                  <Button fx:id="historyButton" mnemonicParsing="false" onAction="#handleHistory" prefHeight="55.0" prefWidth="255.0" text="View Customer History">
                     <font>
                        <Font name="Comic Sans MS" size="18.0" />
                     </font>
                  </Button>
                  <Button fx:id="statusButton" mnemonicParsing="false" onAction="#handleRoomStatus" prefHeight="55.0" prefWidth="255.0" text="View Rooms Status">
                     <font>
                        <Font name="Comic Sans MS" size="18.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <Button fx:id="logoutButton" mnemonicParsing="false" onAction="#handleLogout" text="Log out">
               <font>
                  <Font name="Comic Sans MS" size="18.0" />
               </font>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </center>
   <top>
      <Label text="DATA AND PREFERENCES" BorderPane.alignment="CENTER">
         <font>
            <Font name="Comic Sans MS" size="40.0" />
         </font>
         <BorderPane.margin>
            <Insets left="20.0" right="20.0" top="20.0" />
         </BorderPane.margin>
      </Label>
   </top>
</BorderPane>
