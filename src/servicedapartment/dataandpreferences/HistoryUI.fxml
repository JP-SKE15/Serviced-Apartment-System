            <TableColumn editable="false" maxWidth="1000.0" minWidth="0.0" prefWidth="102.0" resizable="false" text="Code" />
            <TableColumn editable="false" maxWidth="1000.0" prefWidth="78.0" resizable="false" text="Checkout?" />
        </columns>
