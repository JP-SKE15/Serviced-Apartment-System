<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane maxHeight="750.0" maxWidth="1000.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="750.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="servicedapartment.dataandpreferences.HistoryController">
   <top>
      <Label text="HISTORY" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" />
         </BorderPane.margin>
         <font>
            <Font name="Comic Sans MS" size="36.0" />
         </font>
      </Label>
   </top>
   <center>
      <TableView BorderPane.alignment="CENTER">
        <columns>
          <TableColumn editable="false" maxWidth="1000.0" prefWidth="55.0" resizable="false" text="Room" />
          <TableColumn editable="false" maxWidth="1000.0" prefWidth="182.0" resizable="false" text="Customer name" />
            <TableColumn editable="false" maxWidth="1000.0" prefWidth="123.0" resizable="false" text="Phone number" />
            <TableColumn editable="false" maxWidth="1000.0" prefWidth="175.0" resizable="false" text="E-mail" />
            <TableColumn editable="false" maxWidth="1000.0" prefWidth="76.0" resizable="false" text="Stay (days)" />
